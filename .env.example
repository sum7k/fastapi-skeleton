# Environment Variables
# Copy this file to .env and update with your actual values

# JWT Authentication
# Generate a secure random key with: openssl rand -hex 32
JWT_SECRET_KEY=your-super-secret-key-here-minimum-32-characters-long

# Database Configuration
DB_URL=postgresql+asyncpg://user:password@localhost:5432/mydb

# Optional: Additional settings
# DB_POOL_SIZE=5
# DB_MAX_OVERFLOW=10


OTLP_ENDPOINT=http://localhost:4318/v1/traces  # Jaeger OTLP endpoint
